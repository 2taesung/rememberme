{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react-jsx",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@src/*": ["./src/*"],
      "@image/*": ["./public/images/*"],
      "@/*": ["./*"]
    }
  },
  "include": ["src", "lotties"]
}
